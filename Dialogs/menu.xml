<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <default>
    <menu n="100">
       <label>F-16</label>
     <enabled type="bool">true</enabled>
      <item n="1">
      <label>Select Livery</label>
      <binding>
       <command>nasal</command>
       <script>aircraft.livery.dialog.toggle()</script>
      </binding>
     </item>
	 
	 <item n="2">
        <label>MP Shot Report</label>
        <binding>
          <command>nasal</command>
          <script>missile.MPReport()</script>
        </binding>
      </item>
	  
      <item n="3">
        <label>Reload Gun</label>
        <binding>
          <command>nasal</command>
          <script>guns.reload_Cannon()</script>
        </binding>
      </item>
	  
      <item n="4">
        <label>No load</label>
        <binding>
          <command>nasal</command>
          <script>load.clean()</script>
        </binding>
      </item>
	  
	   <item n="5">
        <label>Ferry configuration</label>
        <binding>
          <command>nasal</command>
          <script>load.Ferry()</script>
        </binding>
      </item>
	  
      <item n="6">
        <label>CAP</label>
        <binding>
          <command>nasal</command>
          <script>load.CAP()</script>
        </binding>
      </item>
	  
      <item n="7">
        <label>CAP Extended loiter</label>
        <binding>
          <command>nasal</command>
          <script>load.CAPext()</script>
        </binding>
      </item>
	  
	   <item n="8">
        <label>CAS Bomb run</label>
        <binding>
          <command>nasal</command>
          <script>load.casbomb()</script>
        </binding>
      </item>
     
	 <item n="9">
        <label>CAS Bomb run extender range</label>
        <binding>
          <command>nasal</command>
          <script>load.casbombext()</script>
        </binding>
      </item>
	  
	   <item n="10">
        <label>CAS multi mode</label>
        <binding>
          <command>nasal</command>
          <script>load.casmulti()</script>
        </binding>
      </item>
	  
	 
	  
	   <item n="11">
        <label>Anti Ship</label>
        <binding>
          <command>nasal</command>
          <script>load.ship()</script>
        </binding>
      </item>
	  
	   <item n="12">
        <label>Anti radiation</label>
        <binding>
          <command>nasal</command>
          <script>load.radar()</script>
        </binding>
      </item>
	 
    </menu>
  </default>

</PropertyList>

